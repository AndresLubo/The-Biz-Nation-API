{
  "name": "the-biz-nation",
  "version": "1.0.0",
  "description": "Prueba Backend The Biz Nation",
  "main": "build/index.js",
  "scripts": {
    "build": "npx tsc",
    "start:dev": "nodemon",
    "start": "node build/index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo"
  },
  "keywords": [
    "Node",
    "Express",
    "API",
    "JavaScript",
    "Typescript"
  ],
  "author": "Andres Lubo",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "mysql2": "^3.3.1",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
